<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="10dp" />
            <stroke android:width="0.8dip" android:color="#A59277" />
            <gradient android:angle="-90" android:startColor="#E9D08F" android:endColor="#D6BF83"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="10dp" />
            <stroke android:width="0.8dip" android:color="#A59277" />
            <solid android:color="#D6BF83"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="10dp" />
            <stroke android:width="0.8dip" android:color="#A59277" />
            <gradient android:angle="-90" android:startColor="#E9D08F" android:endColor="#E9D08F" />
        </shape>
    </item>
</selector>